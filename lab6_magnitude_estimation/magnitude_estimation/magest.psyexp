    <Routine name="instrux">
      <TextComponent name="text">
        <Param name="opacity" updates="constant" val="1" valType="code"/>
        <Param name="colorSpace" updates="constant" val="rgb" valType="str"/>
        <Param name="name" updates="None" val="text" valType="code"/>
        <Param name="wrapWidth" updates="constant" val="" valType="code"/>
        <Param name="color" updates="constant" val="white" valType="str"/>
        <Param name="text" updates="constant" val="This PsychoPy code implements a magnitude &amp;#10;estimation experiment.&amp;#10;&amp;#10;In the experiment your will be shown a &amp;#10;&quot;standard&quot; image with quality = 20 and&amp;#10;on each trial you will be asked to &amp;#10;use the keyboard or keypad to enter a  &amp;#10;numeric rating for a &quot;test&quot; image. &amp;#10;&amp;#10;Enter any number that you feel indicates &amp;#10;the quality of the test image relative to &amp;#10;the standard. There are no limits on the &amp;#10;range of values, positive or negative, that&amp;#10;you may use.&amp;#10;&amp;#10;There are 110 trials in the experiment.&amp;#10;&amp;#10;Press the spacebar to start.&amp;#10;" valType="str"/>
        <Param name="stopVal" updates="constant" val="" valType="code"/>
        <Param name="durationEstim" updates="None" val="" valType="code"/>
        <Param name="pos" updates="constant" val="(0, 0)" valType="code"/>
