      <Param name="nReps" updates="None" val="5" valType="code"/>
      <Param name="endPoints" updates="None" val="[0, 1]" valType="num"/>
      <Param name="conditions" updates="None" val="[{'img_id': 1}, {'img_id': 5}, {'img_id': 10}, {'img_id': 15}, {'img_id': 20}, {'img_id': 25}, {'img_id': 30}, {'img_id': 35}, {'img_id': 40}, {'img_id': 45}, {'img_id': 50}, {'img_id': 55}, {'img_id': 60}, {'img_id': 65}, {'img_id': 70}, {'img_id': 75}, {'img_id': 80}, {'img_id': 85}, {'img_id': 90}, {'img_id': 95}, {'img_id': 100}]" valType="str"/>
      <Param name="Selected rows" updates="None" val="" valType="str"/>
    </LoopInitiator>
    <Routine name="trial"/>
    <LoopTerminator name="trials"/>
    <Routine name="thanks"/>
  </Flow>
