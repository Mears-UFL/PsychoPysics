      <CodeComponent name="response_code">
        <Param name="Begin Experiment" updates="constant" val="inputText = &quot;&quot;" valType="extendedCode"/>
        <Param name="name" updates="None" val="response_code" valType="code"/>
        <Param name="Begin Routine" updates="constant" val="theseKeys=&quot;&quot;&amp;#10;shift_flag = False&amp;#10;std_text.alignHoriz = 'center'&amp;#10;test_text.alignHoriz = 'center'&amp;#10;response_text.alignHoriz = 'left'&amp;#10;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="# let's store the final text string into the results finle...&amp;#10;thisExp.addData('subResp', inputText)&amp;#10;inputText=&quot;&quot;" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each Frame" updates="constant" val="n = len(theseKeys)&amp;#10;i = 0&amp;#10;while i &lt; n:&amp;#10;&amp;#10;    # return,enter - end trial, but only after numeric response&amp;#10;    if (theseKeys[i] in ('return','num_enter')):&amp;#10;        if ((inputText != '') &amp; (inputText != '-')):&amp;#10;            continueRoutine = False&amp;#10;            break&amp;#10;        else:&amp;#10;            break&amp;#10;&amp;#10;    # backspace, delete, numlock (clear) - delete the final character on the string&amp;#10;    elif theseKeys[i] in ('backspace','delete','numlock'):&amp;#10;        inputText = inputText[:-1]  &amp;#10;        i = i + 1&amp;#10;&amp;#10;    # minus, num_subtract - toggle '-' as first char in string&amp;#10;    elif theseKeys[i] in ('minus','num_subtract'):&amp;#10;        if (inputText == ''): # if string empty enter -&amp;#10;            inputText += '-'&amp;#10;        elif (inputText[i] != '-'): # if not - enter -&amp;#10;            inputText = '-' + inputText&amp;#10;        elif (inputText[i] == '-'): # if already - remove -&amp;#10;            inputText = inputText[1:]&amp;#10;        i = i + 1&amp;#10;&amp;#10;    # 0-9 - keyboard 0-9, add digit to end of string&amp;#10;    elif (len(theseKeys[0]) == 1):&amp;#10;        inputText += theseKeys[i]&amp;#10;        i = i + 1&amp;#10;&amp;#10;    # num_X - must be keypad 0-9, extract digit, add to end of string&amp;#10;    else:&amp;#10;        key_name = theseKeys[i]&amp;#10;        digit = key_name[4];&amp;#10;        inputText += digit&amp;#10;        i = i + 1&amp;#10;" valType="extendedCode"/>
      </CodeComponent>
